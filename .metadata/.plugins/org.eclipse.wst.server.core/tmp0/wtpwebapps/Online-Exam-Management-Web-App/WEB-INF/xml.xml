     	<servlet-class>RegistrationPackage.AddStudentServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>AddStudentServlet</servlet-name>
     	<url-pattern>/AddStudentServlet</url-pattern>
     </servlet-mapping>
     
         <servlet>
     	<servlet-name>GetAllStudentServlet</servlet-name>
     	<servlet-class>RegistrationPackage.GetAllStudentServlet</servlet-class>
     </servlet>
     <servlet-mapping>

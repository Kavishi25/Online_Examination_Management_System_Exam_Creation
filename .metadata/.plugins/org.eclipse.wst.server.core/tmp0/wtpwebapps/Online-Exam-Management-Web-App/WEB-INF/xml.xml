     </servlet-mapping>
     
     
     
       <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>RegistrationPackage.LoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    
    
      <servlet>
